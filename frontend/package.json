{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:no-turbo": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headless": "cypress run --headless",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "test:all": "npm run test:ci && npm run e2e",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:e2e": "cypress run --headless",
    "test:e2e:dev": "cypress open --e2e",
    "lint:check": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:check:app": "eslint src --ext .ts,.tsx --max-warnings 25 --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.*' --ignore-pattern '**/*.spec.*'",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "type-check": "tsc --noEmit",
    "type-check:tests": "tsc --noEmit --project tsconfig.test.json",
    "type-check:all": "npm run type-check && npm run type-check:tests",
    "check:router": "node scripts/check-router-usage.js",
    "pre-commit": "npm run check:router && npm run lint:check:app && npm run type-check",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/material-nextjs": "^7.3.2",
    "@mui/x-date-pickers": "^8.11.2",
    "@tanstack/react-query": "^5.87.4",
    "axios": "^1.12.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "next": "15.5.3",
    "next-i18next": "^15.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "yup": "^1.7.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.14.6",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "cypress": "^15.2.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node scripts/check-router-usage.js",
      "prettier --write"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
